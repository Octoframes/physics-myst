{"kind":"Notebook","sha256":"9a5fe586da0622ae5a76b9243b1ef98655828c22d7f389b80283d70c21a43843","slug":"wave","dependencies":[],"frontmatter":{"authors":[],"github":"https://github.com/Octoframes/physicsmyst","keywords":[],"exports":[{"format":"ipynb","filename":"wave.ipynb","url":"/physicsmyst/build/wave-4463c3728526b1ce29d795ba79014696.ipynb"}]},"mdast":{"type":"root","children":[{"type":"block","position":{"start":{"line":0,"column":0},"end":{"line":1,"column":0}},"children":[{"type":"code","lang":"python","executable":true,"value":"from manim import *\nconfig.media_embed = True\nparam= \"-v WARNING  --progress_bar None --format=gif  -r  500,200  --disable_caching Example\"\n# param= \"-v WARNING  --progress_bar None   -r  500,200  --disable_caching Example\"","identifier":"nb-cell-0-7b462eb032-code","enumerator":"1","html_id":"nb-cell-0-7b462eb032-code","key":"VFQ8qT1rwe"},{"type":"output","id":"TEqkEm2sSp36YuTwpYTXW","data":[],"identifier":"nb-cell-0-7b462eb032-output","html_id":"nb-cell-0-7b462eb032-output","key":"kcQAcAla0O"}],"data":{"id":"nb-cell-0-7b462eb032","type":"notebook-code"},"identifier":"nb-cell-0-7b462eb032","label":"nb-cell-0-7b462eb032","html_id":"nb-cell-0-7b462eb032","key":"eASOhkZzSp"},{"type":"block","position":{"start":{"line":13,"column":0},"end":{"line":14,"column":0}},"children":[{"type":"code","lang":"python","executable":true,"value":"dispersion = \"high\" # \"high\", \"low\", \"const\"\ndef n(wj):\n    if dispersion == \"high\":\n        return 1 + wj * 0.1\n    if dispersion == \"low\":\n        return 1.1 - wj * 0.01\n    if dispersion == \"const\":\n        return 1\n\nc = 1\nnum_of_waves = 121\nstart_w = 1\nend_w = 7\nx = np.linspace(-2, 10, 1001)\n\ndef g(x, t):\n    u1 = 0\n    for wj in np.linspace(start_w, end_w, num_of_waves):\n        u1 += np.exp(1j * (wj * n(wj) / c * x - t * wj))\n    return u1.real","identifier":"nb-cell-1-822e6434e8-code","enumerator":"2","html_id":"nb-cell-1-822e6434e8-code","key":"uB23MAPP5a"},{"type":"output","id":"iftevV8Zh73qWrac3xNYF","data":[],"identifier":"nb-cell-1-822e6434e8-output","html_id":"nb-cell-1-822e6434e8-output","key":"UGbN8pvPfp"}],"data":{"id":"nb-cell-1-822e6434e8","type":"notebook-code"},"identifier":"nb-cell-1-822e6434e8","label":"nb-cell-1-822e6434e8","html_id":"nb-cell-1-822e6434e8","key":"XWVAezhu7a"},{"type":"block","position":{"start":{"line":43,"column":0},"end":{"line":44,"column":0}},"children":[{"type":"code","lang":"python","executable":true,"value":"class Example(Scene):\n    def construct(self):\n       mob = ParametricFunction(lambda x: [x, g(x,1)*0.03, 0], t_range=[-2, 10],stroke_width=20, color=RED)  \n       self.add(mob.move_to(ORIGIN))\n\n\n%manim $param","identifier":"my-wave1-code","enumerator":"3","html_id":"my-wave1-code","key":"Pypu21lcGO"},{"type":"output","id":"ZXGOi3GI5xnqiX-Zv9aUU","data":[{"output_type":"display_data","metadata":{},"data":{"image/png":{"content_type":"image/png","hash":"4aeb4095ab1cb2872eed7f43ca0dc0a7","path":"/physicsmyst/build/4aeb4095ab1cb2872eed7f43ca0dc0a7.png"},"text/plain":{"content":"<IPython.core.display.Image object>","content_type":"text/plain"}}}],"identifier":"my-wave1-output","html_id":"my-wave1-output","key":"pBsvSMQJGE"}],"data":{"id":"nb-cell-2-13c123c6b8","type":"notebook-code"},"identifier":"my-wave1","label":"my-wave1","html_id":"my-wave1","key":"bvUnYyd4TF"}],"key":"gu4gM4TIzJ"},"references":{"cite":{"order":[],"data":{}}},"footer":{"navigation":{"prev":{"title":"media-notebook","url":"/media-notebook","group":"Minimal Example"}}},"domain":"http://localhost:3000"}